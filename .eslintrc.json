{
  "extends": [ "plugin:@wordpress/eslint-plugin/recommended-with-formatting" ],
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-var": "off",
    "object-shorthand": "off",
    "camelcase": "off",
    "prefer-const": "off",
    "wrap-iife": "off",
    "brace-style": "off",
    "no-redeclare": "off",
    "no-unused-expressions": "off",
    "@wordpress/no-unused-vars-before-return": "off",
    "no-shadow": "off",
    "no-alert": "off",
    "comma-dangle": "off",
    "no-else-return": "off",
    "no-useless-return": "off",
    "no-mixed-operators": "off"
  }
}
