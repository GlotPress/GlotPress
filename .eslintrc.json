{
  "extends": [
    "plugin:@wordpress/eslint-plugin/es5"
  ],
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "camelcase": "off",
    "computed-property-spacing": [
      "error",
      "always"
    ],
    "wrap-iife": "off",
    "no-redeclare": "off",
    "no-unused-expressions": "off",
    "no-shadow": "off",
    "no-alert": "off",
    "no-else-return": "off",
    "no-useless-return": "off",
    "vars-on-top": "off"
  }
}
