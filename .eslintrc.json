{
  "extends": [
    "plugin:@wordpress/eslint-plugin/es5"
  ],
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-var": "off",
    "object-shorthand": "off",
    "camelcase": "off",
    "prefer-const": "off",
    "wrap-iife": "off",
    "no-redeclare": "off",
    "no-unused-expressions": "off",
    "@wordpress/no-unused-vars-before-return": "off",
    "no-shadow": "off",
    "no-alert": "off",
    "no-else-return": "off",
    "no-useless-return": "off",
    "vars-on-top": "off"
  }
}
